  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn run build-css && react-scripts build",
    "build:dev": "env-cmd -f .env.dev yarn build",
    "build:qa": "env-cmd -f .env.qa yarn build",
    "build:prod": "env-cmd -f .env.prod yarn build",
    "test": "react-scripts test --env=jsdom --browser --setupTestFrameworkScriptFile ./src/testSetup.js",
